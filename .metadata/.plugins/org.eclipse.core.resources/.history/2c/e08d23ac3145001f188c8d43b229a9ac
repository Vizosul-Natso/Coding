package patterns;

public class RightTraingle {
	public static void main(String[] args) {
		
		int c = 5, r = 5, s= 0;
		
		for(int i = 0; i < r ; i++) {
			
			for(int j = 0; j<c ; j++) {
				System.out.print("*");
			}
			for(int k = 0; k <= c-i ;k++) {
				System.out.println(" ");
			}
			System.out.println();
		}
	}

}
