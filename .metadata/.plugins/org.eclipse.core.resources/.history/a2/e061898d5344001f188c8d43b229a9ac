package strings;

public class DuplicateCharacters {
	
	public static void main(String[] args) {
		
		String str = "hello";
		
		char arr[] = str.toCharArray();
		int count = 0;
		
		for(int i = 0; i<arr.length ; i++) {
			char a = arr[i];
			System.out.print(a);
			System.out.print("/");
			
			for(int k = i+1; k<arr.length ; k++) {
				char b = arr[k];
				System.out.print(b);
				System.out.print("*");
				if(a==k) {
					count++;
				}
			}
		}
		System.out.println(count);
	}

}
