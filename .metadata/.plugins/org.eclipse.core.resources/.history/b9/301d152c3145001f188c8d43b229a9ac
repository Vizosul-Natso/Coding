package patterns;

public class RightTraingle {
	public static void main(String[] args) {
		
		int c = 5, r = 5, s= 0;
		
		for(int i = 0; i < r ; i++) {
			
			for(int j = 0; j<c ; j++) {
				for( ; s<=c ; s++) {
					System.out.print(" ");
				}
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
