package strings;

import java.util.Scanner;

public class StringwithScanner2 {
	
	public static void main(String[] args) {
		
		String str;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter any string");
		str = sc.next();
		
		System.out.println(str + " " + str.length());
	
	}

}
