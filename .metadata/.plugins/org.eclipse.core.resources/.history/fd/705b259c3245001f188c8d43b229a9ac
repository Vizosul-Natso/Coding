package patterns;

public class RightTraingle {
	public static void main(String[] args) {
		
		int n=5;
		
		for(int i = 0; i < n ; i++) {
			
			for(int x = 0 ; x <= n  ; x++) {
				System.out.print("*");
			}
			for(int y = 0; y <= n-1 ; y++ ) {
				System.out.print(" ");
			}
			
			System.out.println();
		}
	}

}
