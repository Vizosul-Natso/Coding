package Basic;

import java.util.Scanner;

public class NumberSwap {
	public static void main(String[] args) {
		int a, b, temp;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter any two Numbers");
		a = sc.nextInt();
		b = sc.nextInt();
		
		System.out.println(a +" " +b);
		
		b = b - a;
		b = b + a;
		a = a - b;
		a = a + b;
		
		System.out.println(a +" " +b);
		
	}

}
